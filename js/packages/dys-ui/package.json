{
  "name": "dys-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "dist/dys-ui.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dys-ui.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p build:types build-only",
    "build:types": "vue-tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist/types",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.build.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.2",
    "@iconify-json/radix-icons": "^1.2.0",
    "@vueuse/core": "^11.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@vee-validate/zod": "^4.13.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "npm-run-all2": "^6.2.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vite": "^5.4.3",
    "vue": "~3.4.38",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.1.6",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vue": "~3.4.38"
  }
}
