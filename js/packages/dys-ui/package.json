{
  "name": "dys-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "dist/dys-ui.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dys-ui.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p build:types build-only",
    "build:types": "vue-tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist/types",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.build.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.1.200",
    "@iconify-json/radix-icons": "^1.1.14",
    "@vueuse/core": "^10.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "radix-vue": "^1.9.1",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@vee-validate/zod": "^4.13.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "npm-run-all2": "^6.2.2",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vue": "~3.4.29"
  }
}
